{
  "name": "gmzcodes.com",
  "version": "1.0.0",
  "description": "Dani Gamez's personal website!",
  "author": "Dani GÃ¡mez Franco",
  "license": "MIT",
  "main": "index.js",
  "engines": {
    "node": ">= 4.2.1",
    "npm": ">= 3"
  },
  "scripts": {
    "// SCRIPTS //////////////////////////////////////////////////////////////////////////////////////////////////": "",
    "ttab": "ttab",
    "rimraf": "rimraf",
    "tslint": "tslint",
    "typedoc": "typedoc",
    "typings": "typings",
    "webpack": "webpack",
    "jekyll": "jekyll",
    "// CLEAN ////////////////////////////////////////////////////////////////////////////////////////////////////": "",
    "clean": "npm cache clean && npm run rimraf -- node_modules doc typings dist _site",
    "clean:build": "npm run rimraf -- dist _site",
    "clean:webpack": "npm run rimraf -- dist",
    "clean:jekyll": "npm run rimraf -- _site",
    "// CLEAN INSTALL": "*********************************************************************************************",
    "preclean:install": "npm run clean",
    "clean:install": "npm set progress=false && npm install",
    "// CLEAN START": "***********************************************************************************************",
    "preclean:start": "npm run clean",
    "clean:start": "npm start",
    "// WATCH ////////////////////////////////////////////////////////////////////////////////////////////////////": "",
    "watch": "npm run watch:dev",
    "watch:webpack": "npm run build:dev:webpack -- --watch",
    "watch:jekyll": "npm run build:dev:jekyll -- --watch",
    "watch:dev": "npm run ttab npm run build:dev:webpack -- --watch && npm run ttab npm run build:dev:jekyll -- --watch",
    "watch:prod": "npm run ttab npm run build:prod:webpack -- --watch && npm run ttab npm run build:prod:jekyll -- --watch",
    "// BUILD ////////////////////////////////////////////////////////////////////////////////////////////////////": "",
    "build": "npm run build:dev",
    "// BUILD CLEAN": "***********************************************************************************************",
    "prebuild:dev:webpack": "npm run clean:webpack",
    "prebuild:prod:webpack": "npm run clean:webpack",
    "prebuild:dev:jekyll": "npm run clean:jekyll",
    "prebuild:prod:jekyll": "npm run clean:jekyll",
    "// BUILD SHORTCUTS": "*******************************************************************************************",
    "build:webpack": "npm run build:dev:webpack",
    "build:jekyll": "npm run build:dev:jekyll",
    "build:dev": "npm run build:dev:webpack && npm run build:dev:jekyll",
    "build:prod": "npm run build:prod:webpack && npm run build:prod:jekyll",
    "// ACTUAL BUILD COMMANDS": "*************************************************************************************",
    "build:dev:webpack": "cross-env NODE_ENV='dev' && webpack --config __config/webpack.dev.js --progress --profile --colors --display-error-details --display-cached",
    "build:prod:webpack": "cross-env NODE_ENV='prod' && webpack --config __config/webpack.prod.js  --progress --profile --colors --display-error-details --display-cached --bail",
    "build:dev:jekyll": "cross-env NODE_ENV='dev' && jekyll build",
    "build:prod:jekyll": "cross-env NODE_ENV='prod' && jekyll build",
    "/////////////////////////////////////////////////////////////////////////////////////////////////////////////": "",
    "serve": "npm run ttab jekyll serve && npm run ttab npm run watch:webpack",
    "postinstall": "typings install"
  },
  "devDependencies": {
    "autoprefixer-loader": "^3.2.0",
    "awesome-typescript-loader": "^1.1.1",
    "cli-color": "^1.1.0",
    "codelyzer": "0.0.24",
    "common-tags": "^1.3.0",
    "copy": "^0.2.3",
    "cross-env": "^1.0.8",
    "css-loader": "^0.23.1",
    "eslint": "^2.13.1",
    "eslint-config-airbnb-base": "^3.0.1",
    "eslint-plugin-import": "^1.9.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "gulp": "^3.9.1",
    "gulp-debug": "^2.1.2",
    "gulp-help": "^1.6.1",
    "gulp-rename": "^1.2.2",
    "gutil": "^1.6.4",
    "html-webpack-plugin": "^2.21.0",
    "imports-loader": "^0.6.5",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.2",
    "run-sequence": "^1.2.1",
    "sass-loader": "^3.2.1",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.1",
    "ts-loader": "^0.8.2",
    "tslint": "^3.12.1",
    "tslint-loader": "^2.1.5",
    "typedoc": "^0.4.3",
    "typescript": "^1.8.10",
    "typings": "~1.3.0",
    "underscore": "^1.8.3",
    "webpack": "^1.13.0",
    "yargs": "^4.7.1"
  },
  "dependencies": {}
}
